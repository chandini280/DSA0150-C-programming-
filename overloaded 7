#include <iostream>

class Multiplier {
public:
    // Overload the () operator
    int operator()(int a, int b) const {
        return a * b;
    }
};

int main() {
    Multiplier multiply;

    // Call the overloaded () operator
    int result = multiply(5, 4);

    std::cout << "Result of multiplication: " << result << std::endl;

    return 0;
}
